<mapper>
  <cache-ref namespace="memberMapper"/>
  
  <select id="loginMember" resultMap="">
  	select *
  	 from member
  	 where status = 'Y' and user_id = #{userId} and user_pwd = #{userPwd}
  </select>
</mapper>
