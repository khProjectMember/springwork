<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.spring.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.spring.lecture.model.vo.Apply" alias="Apply"/>		
		<typeAlias type="com.kh.spring.lecture.model.vo.Lecture" alias="Lecture"/>		
		<typeAlias type="com.kh.spring.lecture.model.vo.Wish" alias="Wish"/>
		<typeAlias type="com.kh.spring.lecture.model.vo.LectureLocation" alias="LectureLocation"/>
		<typeAlias type="com.kh.spring.lecture.model.vo.Teacher" alias="Teacher" />
		<typeAlias type="com.kh.spring.board.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.kh.spring.board.model.vo.Meeting" alias="Meeting" />
		<typeAlias type="com.kh.spring.board.model.vo.Review" alias="Review" />
		<typeAlias type="com.kh.spring.qa.model.vo.Qna" alias="Qna" />
		<typeAlias type="com.kh.spring.qa.model.vo.QnaReply" alias="QnaReply" />	
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/lecture-mapper.xml" />
		<mapper resource="/mappers/admin-mapper.xml" />
		<mapper resource="/mappers/Board-mapper.xml" />
		<mapper resource="/mappers/Meeting-mapper.xml" />
		<mapper resource="/mappers/Review-mapper.xml" />
		<mapper resource="/mappers/qna-mapper.xml" />
	</mappers>
</configuration>
